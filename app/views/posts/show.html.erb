<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %>
<%= link_to 'Destroy', post_path(@post),
method: :delete,
data: { confirm: 'Are you sure?' } %>
<%= link_to 'Back', posts_path %>
